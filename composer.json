{
    "name": "markocupic/contao-css-style-selector",
    "type": "contao-bundle",
    "description": "Contao 4/5 CSS Style Selector bundle",
    "keywords": [
        "css",
        "classes",
        "selector",
        "contao",
        "modules",
        "content elements"
    ],
    "homepage": "https://github.com/markocupic",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Cupic",
            "homepage": "https://github.com/markocupic/contao-css-style-selector"
        }
    ],
    "support": {
        "issues": "https://github.com/markocupic/contao-css-style-selector/issues",
        "source": "https://github.com/markocupic/contao-css-style-selector"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\ContaoCssStyleSelector\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\ContaoCssStyleSelector\\": "src/"
        }
    }
}
